@{
    @model IEnumerable<CityLibraryFund.Models.Entities.Operation>
    ViewData["Title"] = "Set Rating";
}

<table class="table">
    <tr>
        <td>Id</td>
        <td>Name</td>
        <td>Description</td>
        <td>Date</td>
        <td>UserId</td>
        <td>BookId</td>
    </tr>
    @foreach (var item in Model)
    {
<tr>
    <td>@item.Id</td>
    <td>@item.Name</td>
    <td>@item.Description</td>
    <td>@item.DateAndTime</td>
    <td>@item.UserId</td>
    <td>@item.BookId</td>
</tr>
    }
</table>